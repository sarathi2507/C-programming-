#include<iostream.h>
void main()
{
	int age,n;
        do
        {
	cout<<"Enter your age: ";
	cin>>age;
	if(age>=18)
	{
		cout<<"Personl is eligible for voting"<<endl;
	}
	else
	{
		cout<<"Person is not eligible for voating"<<endl;
	} 
        cout<<"Is there anyone is waiting in the queue to vote. If yes enter 1 another wise enter 0.Thanks for coming"<<endl;
        cin>>n;
       }while(n==1);
}
